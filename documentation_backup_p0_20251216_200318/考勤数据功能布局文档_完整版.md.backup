      // æ·»åŠ å¤„ç†æ—¥å¿—
      if (progress.logs) {
        progress.logs.forEach(log => {
          if (!processingLogs.value.find(l => l.id === log.id)) {
            processingLogs.value.push(log);
          }
        });
      }

      if (progress.status === 'completed') {
        completeMatching(progress.results);
        clearInterval(monitor);

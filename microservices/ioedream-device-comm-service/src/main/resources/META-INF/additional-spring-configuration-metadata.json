{
  "properties": [
    {
      "name": "spring.cloud.nacos.config.server-addr",
      "type": "java.lang.String",
      "description": "Nacos配置中心服务器地址"
    },
    {
      "name": "spring.cloud.nacos.config.namespace",
      "type": "java.lang.String",
      "description": "Nacos配置中心命名空间"
    },
    {
      "name": "spring.cloud.nacos.config.group",
      "type": "java.lang.String",
      "description": "Nacos配置中心分组"
    },
    {
      "name": "spring.cloud.nacos.config.file-extension",
      "type": "java.lang.String",
      "description": "Nacos配置文件扩展名"
    },
    {
      "name": "spring.cloud.nacos.config.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用Nacos配置中心"
    },
    {
      "name": "device.protocol.tcp.port",
      "type": "java.lang.Integer",
      "description": "TCP推送服务器端口"
    },
    {
      "name": "device.protocol.udp.port",
      "type": "java.lang.Integer",
      "description": "UDP推送服务器端口"
    },
    {

          "response": [],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"登录成功\", function() {\n    pm.response.to.have.status(200);\n    const jsonData = pm.response.json();\n    pm.expect(jsonData.code).to.eql(200);\n    pm.expect(jsonData.data).to.have.property('token');\n    pm.expect(jsonData.data).to.have.property('refreshToken');\n    \n    // 保存token到集合变量\n    pm.collectionVariables.set('jwtToken', jsonData.data.token);\n});"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },

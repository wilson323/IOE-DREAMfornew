  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8080",
      "description": "API基础地址"
    },
    {
      "key": "jwtToken",
      "value": "",
      "description": "JWT认证Token"
    },
    {
      "key": "contentType",
      "value": "application/json",
      "description": "内容类型"
    }
  ],
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "Authorization",
        "value": "Bearer {{jwtToken}}",
        "type": "string"
      }
    ]
  },
  "event": [
    {
      "listen": "prerequest",
      "script": {
        "exec": [
          "// 自动设置认证头\nif (pm.collectionVariables.get('jwtToken')) {\n    pm.request.headers.add({\n        key: 'Authorization',\n        value: 'Bearer ' + pm.collectionVariables.get('jwtToken')\n    });\n}\n\n// 设置内容类型\nif (!pm.request.headers.has('Content-Type')) {\n    pm.request.headers.add({\n        key: 'Content-Type',\n        value: pm.collectionVariables.get('contentType')\n    });\n}"
        ],
        "type": "text/javascript"
      }
    }
  ],
